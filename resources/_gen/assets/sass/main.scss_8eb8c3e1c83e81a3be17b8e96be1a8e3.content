@charset "UTF-8";body{width:960px;margin:0 auto;font-family:-apple-system,system-ui,BlinkMacSystemFont,segoe ui,Helvetica,sans-serif,apple color emoji,segoe ui emoji,segoe ui symbol;background-color:#f9f4ec;color:#6d4c92;font-size:18px}main{margin:0 0 35px}nav ul{list-style:none;padding:30px 5px;border-bottom:2px solid #6d4c92;font-size:20px}nav ul li{display:inline;padding:0 1rem 0 0}footer{padding:2px;font-size:.7em;padding-top:35px;border-top:2px solid #6d4c92}h1,strong{font-weight:600}h2,h3,h4,h5,h6{font-weight:400}h1{font-size:3.1rem;line-height:3.2rem;column-span:all}h2{font-size:2.2rem;line-height:2.5rem;column-span:all}img{width:100%}a{color:#6d4c92;text-decoration:none}a:hover{color:red}p{margin:0 0 1em}.grid{columns:2}li img{width:100%}